load: { ...item, quantity },\n    });\n  };\n\n  return (\n    <Card>\n      <Link to={`/product/${product.id}`}>\n        <img src={product.image} className=\"card-img-top\" alt={product.name} />\n      </Link>\n      <Card.Body>\n        <Link to={`/product/${product.id}`}>\n          <Card.Title>{product.title}</Card.Title>\n        </Link>\n        <Rating rating={product.rating} numReviews={product.numReviews} />\n        <Card.Text>Category: {product.category}</Card.Text>\n        <Card.Text>${product.price}</Card.Text>\n        {product.countInStock === 0 ? (\n          <Button variant=\"light\" disabled>\n            Out of stock\n          </Button>\n        ) : (\n          <Button onClick={() => addToCartHandler(product)}>Add to cart</Button>\n        )}\n      </Card.Body>\n    </Card>\n  );\n}\nexport default Product;\n","/Users/grantwilliams/Downloads/Grace shopper skate shop/frontend/src/components/Rating.js",[],[],"/Users/grantwilliams/Downloads/Grace shopper skate shop/frontend/src/components/MessageBox.js",[],[],"/Users/grantwilliams/Downloads/Grace shopper skate shop/frontend/src/components/CheckoutSteps.js",[],[],{"ruleId":"176","severity":1,"message":"177","line":50,"column":10,"nodeType":"178","messageId":"179","endLine":50,"endColumn":20},{"ruleId":"176","severity":1,"message":"180","line":1,"column":40,"nodeType":"178","messageId":"179","endLine":1,"endColumn":48},{"ruleId":"176","severity":1,"message":"181","line":9,"column":8,"nodeType":"178","messageId":"179","endLine":9,"endColumn":14},{"ruleId":"176","severity":1,"message":"182","line":12,"column":8,"nodeType":"178","messageId":"179","endLine":12,"endColumn":14},{"ruleId":"176","severity":1,"message":"183","line":14,"column":8,"nodeType":"178","messageId":"179","endLine":14,"endColumn":21},{"ruleId":"176","severity":1,"message":"184","line":40,"column":9,"nodeType":"178","messageId":"179","endLine":40,"endColumn":17},{"ruleId":"185","severity":1,"message":"186","line":71,"column":6,"node